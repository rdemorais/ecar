<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ecar.pojo.StatusRelatorioSrl" 
    table="TB_STATUS_RELATORIO_SRL"
    lazy="false"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="codSrl"
        type="java.lang.Long"
        column="COD_SRL"
    >
        <generator class="increment" />
    </id>

    <property
        name="indAtivoSrl"
        type="java.lang.String"
        column="IND_ATIVO_SRL"
        length="1"
    />
    <property
        name="descricaoSrl"
        type="java.lang.String"
        column="DESCRICAO_SRL"
        length="20"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to AcompReferenciaItemAri -->
    <set
        name="acompReferenciaItemAris"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="COD_SRL" />
        </key>
        <one-to-many 
            class="ecar.pojo.AcompReferenciaItemAri"
        />
    </set>

</class>
</hibernate-mapping>
