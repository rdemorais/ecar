			</plugin>

			<plugin>
				<artifactId>maven-release-plugin</artifactId>
				<configuration>
					<encoding>${encoding.default}</encoding>
					<tagBase>HEAD</tagBase>
				</configuration>
			</plugin>

			<plugin>
				<artifactId>maven-resources-plugin</artifactId>
				<configuration>
					<encoding>${encoding.default}</encoding>
				</configuration>
			</plugin>

